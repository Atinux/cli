{
  "name": "@dimerapp/cli",
  "version": "1.0.4",
  "author": "virk",
  "bin": {
    "dimer": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/dimerapp/cli/issues"
  },
  "dependencies": {
    "@dimerapp/assets-middleware": "^1.0.0",
    "@dimerapp/cli-utils": "^1.0.3",
    "@dimerapp/config-parser": "^2.0.2",
    "@dimerapp/context": "^2.0.0",
    "@dimerapp/datastore": "^3.0.0",
    "@dimerapp/dfile": "^1.1.1",
    "@dimerapp/fs-client": "^2.0.0",
    "@dimerapp/http-server": "^2.0.1",
    "@dimerapp/image": "^1.0.2",
    "@oclif/command": "^1.5.0",
    "@oclif/config": "^1.7.4",
    "@oclif/plugin-help": "^2.1.1",
    "pretty-bytes": "^5.1.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@adonisjs/mrm-preset": "^1.0.11",
    "@oclif/dev-cli": "^1.18.0",
    "@oclif/tslint": "^2.0.0",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "cz-conventional-changelog": "^2.1.0",
    "globby": "^8.0.1",
    "intercept-stdout": "^0.1.2",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dimerapp/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dimer",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "com.dimerapp.cli"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimerapp/cli.git"
  },
  "scripts": {
    "mrm": "mrm --preset @adonisjs/mrm-preset",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "prepare": "rm -rf lib && tsc",
    "pack": "oclif-dev pack",
    "test": "japa",
    "test:win": "node ./node_modules/japa-cli/index.js",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "description": "Simplest way to publish docs",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
