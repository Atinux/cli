    "@dimerapp/context": "^1.0.1",
    "@dimerapp/datastore": "^1.0.18",
    "@dimerapp/dfile": "^1.0.11",
    "@dimerapp/fs-client": "^1.0.11",
    "@dimerapp/http-server": "^1.0.6",
    "@dimerapp/image": "^1.0.2",
    "@oclif/command": "^1.4.36",
    "@oclif/config": "^1.6.33",
    "@oclif/plugin-help": "^2.0.5",
    "pretty-bytes": "^5.1.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@adonisjs/mrm-preset": "^1.0.10",
    "@oclif/dev-cli": "^1.15.4",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.5",
    "globby": "^8.0.1",
    "intercept-stdout": "^0.1.2",
